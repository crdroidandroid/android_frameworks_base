<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Description of the Random button in a KeyboardView. [CHAR LIMIT=NONE] -->
    <string name="keyboardview_keycode_random">Перезагрузить произвольные кнопки</string>
    
    <!-- On the unlock gesture screen, shown at the top of the unlock screen to tell the user what to do. Below this text is the place for the user to draw the gesture. -->
    <string name="lockscreen_gesture_instructions">Нарисуйте жест для разблокировки</string>
    <!-- Shown to confirm that the user entered their lock gesture correctly. -->
    <string name="lockscreen_gesture_correct">Правильно!</string>
    <!-- On the unlock gesture screen, shown when the user enters the wrong lock gesture and must try again. -->
    <string name="lockscreen_gesture_wrong">Попробуйте снова</string>
    <!-- Accessibility description of the gesture unlock. [CHAR_LIMIT=none] -->
    <string name="keyguard_accessibility_gesture_unlock">Жест разблокировки.</string>
    <!-- Accessibility description of the unlock gesture area. [CHAR_LIMIT=none] -->
    <string name="keyguard_accessibility_gesture_area">Область для жеста.</string>
    <!-- Message shown in gesture unlock after some number of unsuccessful attempts -->
    <string name="kg_forgot_gesture_button_text">Забыли жест</string>
    <!-- Message shown when user enters wrong pattern -->
    <string name="kg_wrong_gesture">Неправильный жест</string>
    <!-- Instructions for using the gesture unlock screen -->
    <string name="kg_gesture_instructions">Нарисуйте ваш жест</string>
    <string name="kg_too_many_failed_gesture_attempts_dialog_message">
        Вы неправильно нарисовали жест разблокировки <xliff:g id="number">%d</xliff:g> раз(а).
        \n\nПовторите попытку через <xliff:g id="number">%d</xliff:g> секунд.
    </string>

</resources>
